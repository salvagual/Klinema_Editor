 {

  "name": "Klinema editor",
  "short_name": "Klinema_1_editor",
  "description": "Ayuda a preparar los filtros para Klinema",
  "version": "3.8.9",
  "manifest_version": 3,
  "permissions": ["storage"], 
  "icons": {
    "24": "img/icon24.png",
    "38": "img/icon38.png",
    "48": "img/icon48.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  },
  "action":{
    "default_icon": {
      "24": "img/icon24.png",
      "38": "img/icon38.png",
      "48": "img/icon48.png",
      "64": "img/icon64.png",
      "128": "img/icon128.png"
    }
  },
  "web_accessible_resources": 
	[
		{
			"resources":["img/icon64.png", "img/OK64.png", "img/NO64.png", "img/cargando.gif", "go2netflix.js"],
			"matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": [
        "https://klinema.com/*",
        "https://www.netflix.com/*",
        "https://www.primevideo.com/*",
        "https://www.filmin.es/*",
	"https://ver.movistarplus.es/*",
	"https://play.google.com/*",
	"https://www.disneyplus.com/*",
	"https://play.max.com/*",
	"https://play.hbomax.com/*",
	"https://www.rakuten.tv/*",
	"https://tv.apple.com/*",
	"https://ver.famiplay.com/*",
	"https://ver.acontraplus.com/*"
      ],
      "js": [
        "klinema.js"
      ],
	"run_at": "document_idle"
    }
  ]
}